    stage('Checkout') {
        dir('pipeline-repo') {
            git branch: 'master', url: 'https://github.com/EdMan1022/jenkins-scrap-pipeline.git'
        }
    }
    
    baseImagePackage = load 'pipeline-repo/src/main/groovy/pipelines/utilities.pipeline'
